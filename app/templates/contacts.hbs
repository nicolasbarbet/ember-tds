<h3>Contacts</h3>



<div class="container">
  <div class="panel panel-primary">
    <div class="panel-heading"><h1>Contacts</h1></div>
    <div class="container-fluid">
      <Input value={{model.filter}} placeholder="Filtrer"/>
      {{#each model.datas as |contact|}}
        <br>
        <span>{{contact.nom}} {{contact.prenom}} {{contact.email}}</span>
        <button>Valider</button>
        <LinkTo class="" @route="contacts.edit" @model={{contact}}>
        </LinkTo>

      {{/each}}
    </div>
  </div>
  
</div>

{{outlet}}